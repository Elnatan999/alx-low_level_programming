#include <stdio.h>
#include <string.h>
/**
 * main - print reverse
 * Return: 0
 */
int main(void)
{
const char word;
const char x;
const char end;
for (word = 'a'; word <= 'z'; word++)
end = strlen[word] - 1;
for (x = end; x >= 0; x--)
putchar(word[x]);
putchar('\n');
/**
 * main - print reverse
 * Return: 0
 */
return (0);
}
  
